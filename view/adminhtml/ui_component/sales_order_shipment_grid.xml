<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="print_tourmix_shipping_label">
                <settings>
                    <url path="tourmix/shipment/massPrintShippingLabel"/>
                    <type>print_tourmix_shipping_label</type>
                    <label translate="true">Print Tourmix Shipping Labels</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
